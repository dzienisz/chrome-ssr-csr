{
  "manifest_version": 3,
  "name": "CSR vs SSR Detector",
  "version": "2.0",
  "description": "Determine if a page is Client-Side Rendered or Server-Side Rendered.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.webp",
      "48": "icon.webp",
      "128": "icon.webp"
    }
  },
  "background": {
    "service_worker": "backgroundV2.js"
  }
}
