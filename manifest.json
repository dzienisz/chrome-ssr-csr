{
  "manifest_version": 3,
  "name": "CSR vs SSR Detector",
  "version": "1.0",
  "description": "Determine if a page is Client-Side Rendered or Server-Side Rendered.",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
